<div class="row">
    <ul class="mk-movie-dt-nav-wrapper">
        <li>
            <a href=<%= PathDict.GET.MOVIE_DETAILS + "?id=" + movie.movieId%>>OVERVIEW</a>
        </li>
        <li>
            <a href=<%= PathDict.GET.MOVIE_DETAILS_SCHEDULES + "?id=" + movie.movieId%>>TICKETS</a>
        </li>
        <li>
            <a href=<%= PathDict.GET.MOVIE_DETAILS_REVIEWS + "?id=" + movie.movieId%>>MOVIE REVIEWS</a>
        </li>
        <% if(userInfo.role === Roles.USER ) {%>
            <li>
                <a href=<%= PathDict.GET.MOVIE_DETAILS_POST_REVIEW + "?id=" + movie.movieId%>>POST REVIEW</a>
            </li>
        <% }%>
        <% if(userInfo.role === Roles.HAD ) {%>
            <li>
                <a href=<%= PathDict.GET.MOVIE_DETAILS_ORDERS + "?id=" + movie.movieId%>>VIEW ORDERS</a>
            </li>
        <% }%>
    </ul>
</div>